{
  "manifest_version": 3,
  "name": "Metacat",
  "version": "1.0",
  "description": "Popup SEO and analytics companion fast stats of current page",
  "icons": {
    "16": "assets/icons/spacecat16.png",
    "48": "assets/icons/spacecat48.png",
    "128": "assets/icons/spacecat128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Metacat will fetch all growth related data you need",
    "default_icon": {
      "16": "assets/icons/spacecat16.png"
     
    }
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "webRequest",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
